= form_for @case do |f|
  - if @case.errors.any?
    #error_explanation
      %h2= "#{pluralize(@case.errors.count, "error")} prohibited this case from being saved:"
      %ul
        - @case.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :string
    = f.text_field :string
  .field
    = f.label :string
    = f.text_field :string
  .actions
    = f.submit 'Save'
